<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="400.0" prefWidth="600.0" style="-fx-padding: 3cm" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Customer" stylesheets="@font.css">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Label text="username" GridPane.columnIndex="1" GridPane.rowIndex="2" />
       <TextField fx:id="userName" editable="false" GridPane.columnIndex="2" GridPane.rowIndex="2" />
      <Label text="firstName" GridPane.columnIndex="1" GridPane.rowIndex="3" />
       <TextField fx:id="firstName" GridPane.columnIndex="2" GridPane.rowIndex="3" />
      <Label text="lastName" GridPane.columnIndex="1" GridPane.rowIndex="4" />
       <TextField fx:id="lastName" GridPane.columnIndex="2" GridPane.rowIndex="4" />
      <Label text="passWord" GridPane.columnIndex="1" GridPane.rowIndex="5" />
       <TextField fx:id="passWord" GridPane.columnIndex="2" GridPane.rowIndex="5" />
      <Label text="number" GridPane.columnIndex="1" GridPane.rowIndex="6" />
       <TextField fx:id="phoneNumber" GridPane.columnIndex="2" GridPane.rowIndex="6" />
      <Label text="mail" GridPane.columnIndex="1" GridPane.rowIndex="7" />
       <TextField fx:id="mail" GridPane.columnIndex="2" GridPane.rowIndex="7" />
      <Label text="role" GridPane.columnIndex="1" GridPane.rowIndex="8" />
       <TextField fx:id="role" editable="false" GridPane.columnIndex="2" GridPane.rowIndex="8" />
       <Label text="credit" GridPane.columnIndex="1" GridPane.rowIndex="9" />
       <TextField fx:id="credit" editable="false" GridPane.columnIndex="2" GridPane.rowIndex="9" />
       <Label text="offCodes" GridPane.columnIndex="1" GridPane.rowIndex="10" />
