<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.UsersManaging">
    <children>
        <TableView fx:id="table" layoutX="7.0" layoutY="9.0" prefHeight="286.0" prefWidth="590.0">
            <columns>
                <TableColumn fx:id="userName" editable="false" prefWidth="75.0" text="OffCode barcode" />
            </columns>
        </TableView>
        <TextField fx:id="user" layoutX="7.0" layoutY="305.0" prefHeight="25.0" prefWidth="68.0" promptText="userName" />
        <TextField fx:id="pass" layoutX="105.0" layoutY="305.0" prefHeight="25.0" prefWidth="68.0" promptText="passWord" />
        <Button fx:id="addButton" layoutX="534.0" layoutY="305.0" mnemonicParsing="false" onAction="#add" text="Add" />
      <Button fx:id="remove" onAction="#remove" layoutX="527.0" layoutY="340.0" mnemonicParsing="false" text="remove" />
    </children>

</AnchorPane>
