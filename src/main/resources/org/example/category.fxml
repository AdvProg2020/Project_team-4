<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Category">

    <children>
        <TableView fx:id="table" editable="true" layoutX="7.0" layoutY="9.0" prefHeight="286.0" prefWidth="590.0">
            <columns>
                <TableColumn fx:id="nameColumn" onEditCommit="#changeName" prefWidth="75.0" text="name" />
                <TableColumn fx:id="tagsColumn" onEditCommit="#changeTags" prefWidth="75.0" text="tags" />
                <TableColumn fx:id="productsColumn" onEditCommit="#changeProducts" prefWidth="75.0" text="products" />
                <TableColumn fx:id="subCategoriesColumn" onEditCommit="#changeSubCategories" prefWidth="75.0" text="subCategories" />
            </columns>
        </TableView>
        <TextField fx:id="name" layoutX="7.0" layoutY="305.0" prefHeight="25.0" prefWidth="68.0" promptText="name" />
        <TextField fx:id="tags" layoutX="105.0" layoutY="305.0" prefHeight="25.0" prefWidth="68.0" promptText="tas" />
        <TextField fx:id="subCategories" layoutX="200.0" layoutY="305.0" prefHeight="25.0" prefWidth="68.0" promptText="subCategory" />
        <TextField fx:id="products" layoutX="300.0" layoutY="305.0" prefHeight="25.0" prefWidth="68.0" promptText="products" />
        <Button fx:id="add" layoutX="534.0" layoutY="305.0" mnemonicParsing="false" onAction="#add" text="Add" />
        <Button fx:id="remove" layoutX="527.0" layoutY="340.0" mnemonicParsing="false" onAction="#remove" text="remove" />
      <Button layoutX="15.0" layoutY="365.0" mnemonicParsing="false" onAction="#switchtoAccountPage" text="Account" />
    </children>

</AnchorPane>
