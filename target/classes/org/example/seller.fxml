<GridPane prefHeight="400.0" prefWidth="600.0" style="-fx-padding: 3cm" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Seller" stylesheets="@font.css">
    <columnConstraints>
        <ColumnConstraints />
