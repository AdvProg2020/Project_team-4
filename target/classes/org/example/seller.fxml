        <Button onAction="#logout" mnemonicParsing="false" text="logout" GridPane.rowIndex="11" GridPane.columnIndex="12"/>
        <Button onAction="#goToMainPage" mnemonicParsing="false" text="main" GridPane.rowIndex="11" GridPane.columnIndex="13"/>
        <Button onAction="#goToCategoryPage" mnemonicParsing="false" text="categories" GridPane.rowIndex="11" GridPane.columnIndex="14"/>
        <ImageView fx:id="imageView" fitHeight="130.0" fitWidth="125.0" GridPane.columnIndex="2" GridPane.halignment="CENTER" />
        <Button fx:id="chooseImage" onAction="#chooseImage" text="select profileImage" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
