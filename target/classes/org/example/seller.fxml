        <TextField fx:id="mail" GridPane.columnIndex="1" GridPane.rowIndex="7" />
        <Label text="role" GridPane.rowIndex="8" />
        <TextField fx:id="role" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="8" />
        <Label text="credit" GridPane.rowIndex="9" />
        <TextField fx:id="credit" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="9" />
        <Label text="company" GridPane.rowIndex="10" />
        <TextField fx:id="companyField" editable="true" GridPane.columnIndex="1" GridPane.rowIndex="10" />
        <Button fx:id="saveButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="76.0" text="save" GridPane.columnIndex="8" GridPane.rowIndex="11" />
        <Button fx:id="products" onAction="#goToProductsPage" mnemonicParsing="false" text="products" GridPane.rowIndex="11" GridPane.columnIndex="9"/>
